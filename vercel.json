{
	"version": 2,
	"builds": [
	  {
		"src": "api/index.ts",
		"use": "@vercel/node",
		"config": { "includeFiles": ["public/**/*"] }
	  }
	],
	"routes": [
	  { "src": "/", "dest": "/api/index.ts" },
	  { "src": "/test", "dest": "/api/index.ts" },
	  { "src": "/admin", "dest": "/api/index.ts" }
	]
  }
  