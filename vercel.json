{
	"version": 2,
	"builds": [
	  {
		"src": "index.ts",
		"use": "@vercel/node"
	  },
	  {
		"src": "public/**/*",
		"use": "@vercel/static"
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "/index.ts"
	  },
	  {
		"src": "/test",
		"dest": "/public/test.html"
	  },
	  {
		"handle": "filesystem"
	  },
	  {
		"src": "/(.*)",
		"dest": "/public/$1"
	  }
	]
  }
  